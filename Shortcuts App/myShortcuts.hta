<!DOCTYPE html>
<html>
<head>
     <title>myShortcuts
          </title>
     <HTA:APPLICATION
          ID="myShortcuts"
          APPLICATIONNAME="myShortcuts"
          SCROLL="no"
          SINGLEINSTANCE="yes"
          WINDOWSTATE="normal"
          BORDER="thick"
          SysMenu="yes"
          />
     <script language="VBScript">
     OPTION Explicit
     Public param 
     Public initDTp
     Public debg
     param=Split(myShortcuts.commandLine, Chr(34))
     debg="no"  '  "yes" to debug'
     If debg = "yes" Then
          MsgBox myShortcuts.commandLine & vbcrlf & _
               "No. params: " & UBound(param) & _
               vbcrlf & "0: """ & param(0) & """" & _
               vbcrlf & "1: """ & param(1) & """" & _
               vbcrlf & "2: """ & param(2) & """" ' & _ '
               ' vbcrlf & "3: """ & param(3) & """" & _ '
               ' vbcrlf & "4: """ & param(4) & """" '
     End If

     If param(2) = " " Then
          If debg = "yes" Then
               MsgBox "There is no ""Table"" as a parameter"
          End If
          initDTp=" "
     Else
          initDTp = trim(param(2))
          If debg = "yes" Then
               MsgBox """" & initDTp & """"
          End If
     End If
          </script>

     <script type="text/javascript" src="http://ajax.googleapis.com/ajax/libs/jquery/1/jquery.min.js">
          </script>

     <script type="text/javascript" src="http://code.jquery.com/ui/1.10.3/jquery-ui.js">
          </script>

     <script type="text/javascript" src=".\myShortcuts\myShortcuts-js.js">
          </script>

     <script type="text/vbscript" src=".\myShortcuts\myShortcuts-vbs.vbs">
          </script>

     <link rel="stylesheet" type="text/css" href=".\myShortcuts\myShortcuts-style.css" />

     </head>

<body id=myShortcuts>

<section id=secTimers>
<div id=t01 class=divTimers>

     <div class=timersLeft>
     <button id=starttimerB class=bigBut>Start</button>
     <button id=stoptimerB class=bigBut>Stop</button>
          </div>

     <div class=timersMid>
     <span id=tagArea>Count</span>&nbsp;&nbsp;<span id=msgArea>Status</span>
          </div>

     <div class=timersRight>
     <button id=moreoptionsB class=bigBut>More...</button>
     <button id=cleartimerB class=bigBut>Hide</button>
          </div>

     <hr id=timers class=breakSec />

     </div>
     </section>


<section id=secAdditem>
<div id=divAdditem>

     <div id=additemLeft>
          </div>

     <div id=additemMid>
     <label for=additemShort id=additemsLab>Short Form:</label>
     <input type=text id=additemShort accessKey="h" />
          </div>

     <div id=additemRight>
     <button id=etitbigB class=bigBut>Edit</button>
     <button id=additemB class=bigBut>Add</button>
     <button id=clritemB class=bigBut>Clear</button>
          </div>

     <div id=additemBot>
     <label for=additemFull id=additemfLab>Long Form:</label>
     <textarea id=additemFull accessKey="l" rows=3></textarea>
          </div>

     <hr id=additem class=breakSec />

     </div>
     </section>


<section id=secButrow>
<div id=divButrow>

     <label for=butrowLeft id=butrowLeftLab>Buttons:</label>
     <div id=butrowLeft>
     Buttons
          </div>

     <div id=butrowRight>
     <label for=datatblSel id=datatblSelLab>Data Table:</label>
     <select id=datatblSel accessKey="t"></select>
          </div>

     <hr id=butrow class=breakSec />

     </div>
     </section>


<section id=secDatatable>
<div id=divDataout>

     <div id=divDatatable>
     Data Table
          </div>

     </div>
     </section>

</body>
</html>
